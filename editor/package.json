{
  "name": "shadow-electorn-starter",
  "version": "1.0.0",
  "description": "panaeolus simple electron-ui",
  "main": "resources/main.js",
  "scripts": {
    "dev": "./node_modules/.bin/shadow-cljs watch main renderer",
    "build": "./node_modules/.bin/shadow-cljs compile main renderer",
    "release": "shadow-cljs release main renderer",
    "package": "electron-packager . panaeolus --out dist/ --extra-resource resources/main.js --overwrite",
    "clean": "rm -rf resources/public/js/* && rm -rf target"
  },
  "keywords": [
    "shadow-cljs",
    "reagent",
    "electron"
  ],
  "author": "Hlöðver Sigurðsson <hlolli@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "shadow-cljs": "^2.8.37"
  },
  "dependencies": {
    "ace-builds": "^1.4.4",
    "ajv": "6.10.0",
    "async-exit-hook": "^2.0.1",
    "bencode": "^2.0.1",
    "brace": "^0.11.1",
    "create-react-class": "^15.6.3",
    "node-jre": "^0.2.3",
    "react": "^16.8.6",
    "react-ace": "^6.5.0",
    "react-dom": "^16.8.6",
    "react-highlight": "^0.12.0",
    "react-split-pane": "^0.1.87",
    "react-virtualized": "^9.21.1",
    "simplebar-react": "^1.0.0",
    "uri-js": "^4.2.2",
    "vex-js": "^4.1.0"
  }
}
